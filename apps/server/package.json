{
  "name": "@webtex/server",
  "version": "1.0.0",
  "keywords": [],
  "license": "ISC",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "wipe": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "clean": "rm -rf .turbo && rm -rf dist",
    "build": "pnpm clean && tsup src/index.ts && pnpm cpstatic",
    "cpstatic": "cp -R './src/public/' './dist/public/'",
    "dev": "tsup src/index.ts --watch --onSuccess \"pnpm cpstatic && node dist/index.js\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "NODE_OPTIONS='--experimental-specifier-resolution=node' node dist/index.js",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.231.0",
    "@aws-sdk/s3-request-presigner": "^3.231.0",
    "@types/connect-redis": "^0.0.19",
    "@webtex/lib": "workspace:*",
    "@webtex/list": "workspace:*",
    "@webtex/string": "workspace:*",
    "bcrypt": "^5.1.0",
    "concurrently": "^7.6.0",
    "connect-redis": "5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "ioredis": "^5.2.4",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "kysely": "^0.22.0",
    "nanoid": "^4.0.0",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "tsup": "^6.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.14",
    "@types/nodemailer": "^6.4.6",
    "@types/pg": "^8.6.5",
    "@webtex/shared": "workspace:*",
    "date-fns": "^2.29.3",
    "esbuild": "^0.16.6",
    "nodemon": "^2.0.20",
    "npm-dts": "^1.3.12",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  }
}

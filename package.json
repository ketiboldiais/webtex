{
  "name": "@webtex/root",
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "structs/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache --parallel --continue",
